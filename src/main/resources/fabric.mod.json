{
  "schemaVersion": 1,
  "id": "softcore-addon",
  "version": "${version}",
  "name": "Softcore Addon",
  "description": "A softcore addon for Meteor Client by 8oft.",
  "authors": [
    "8oft"
  ],
  "contact": {
    "sources": "https://github.com/8oft/meteor-softcore-addon"
  },
  "icon": "assets/softcore/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.softcore.addon.SoftcoreAddon"
    ]
  },
  "mixins": [
    "softcore-addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
